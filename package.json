{
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix dev -c \"npm run start\"",
    "dev": "concurrently \"npm run sass\" \"npm run dev:remix\"",
    "start": "wrangler pages dev --live-reload --compatibility-date=2023-08-14 ./public ",
    "typecheck": "tsc",
    "sass": "sass --watch app/styles/sass/styles.scss app/styles/styles.css --style compressed",
    "sass:build": "sass app/styles/sass/styles.scss app/styles/styles.css --style compressed",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- public/img",
    "lint:fix": "oxlint --fix && eslint --cache --fix \"**/*.ts*\"",
    "lint": "oxlint && eslint --cache \"**/*.ts*\""
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.28",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@remix-run/cloudflare": "^2.10.2",
    "@remix-run/cloudflare-pages": "^2.10.2",
    "@remix-run/css-bundle": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.1",
    "isbot": "^5.1.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-svg": "^16.1.34",
    "tw-elements": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/eslint-config": "^2.10.2",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.7",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "wrangler": "^3.64.0",
    "oxlint": "^0.6.0"
  },
  "engines": {
    "node": ">=22.2.0"
  }
}
