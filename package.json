{
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix dev -c \"npm run start\"",
    "dev": "concurrently \"npm run sass\" \"npm run dev:remix\"",
    "start": "wrangler pages dev --live-reload --compatibility-date=2023-08-14 ./public ",
    "typecheck": "tsc",
    "sass": "sass --watch app/styles/sass/styles.scss app/styles/styles.css --style compressed",
    "sass:build": "sass app/styles/sass/styles.scss app/styles/styles.css --style compressed",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- public/img",
    "lint:fix": "biome check --write .",
    "lint": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@remix-run/cloudflare": "^2.17.0",
    "@remix-run/cloudflare-pages": "^2.17.0",
    "@remix-run/css-bundle": "^2.17.0",
    "@remix-run/react": "^2.17.0",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.1",
    "isbot": "^5.1.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-svg": "^16.2.0",
    "tw-elements": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@remix-run/dev": "^2.17.0",
    "@remix-run/eslint-config": "^2.17.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "wrangler": "^3.99.0"
  },
  "engines": {
    "node": ">=24.8.0"
  }
}
